<%= render 'user', user: @user %>
<%= render 'books/book', book_new: @book_new %>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="1"></th>
    </tr>
  </thead>
<% @users.each do |user| %>
<tbody>
  <td><%= image_tag user.get_profile_image(100,100) %></td>
  <td><%= user.name %></td>
  <td><%= link_to "Show", user_path(@user.id) %></tb>
<% end %>
</table>